<asp:Content ID="Content3" ContentPlaceHolderID="head" runat="Server">
    <script type="text/javascript" src="../SmartEditor/js/service/HuskyEZCreator.js"></script>
    <script type="text/javascript">
        var editor_object = [];
        $(function () {
            //전역변수선언

            nhn.husky.EZCreator.createInIFrame({
                oAppRef: editor_object,
                elPlaceHolder: "txtContent",
                sSkinURI: "../Smarteditor/SmartEditor2Skin.html",
                htParams: { fOnBeforeUnload: function () { } }

            });

            var ddlEmail = $("#<%=ddlEmail3.ClientID %>");
            var txtEmail2 = $("#<%=txtEmail2.ClientID %>");
            //이메일 선택
            ddlEmail.change(function () {
                var selectedVal = $('#<%=ddlEmail3.ClientID %> option:selected').val();
                if (selectedVal == 'direct') {
                    txtEmail2.val('');
                }
                else {
                    txtEmail2.val(selectedVal);
                }
            });
        })

        function fnValidation() {

            var txtCompanyName = $("#<%=txtCompanyName.ClientID%>");
            var txtBoardWrite = $("#<%=txtBoardWrite.ClientID%>");
            var txtTitle = $("#<%=txtTitle.ClientID%>");
            var hfContent = $("#<%=hfContent.ClientID%>");
            var txtEmail1 = $("#<%=txtEmail1.ClientID%>");
            var txtEmail2 = $("#<%=txtEmail2.ClientID%>");

            <%--var txtSelPhone1 = $("#<%=txtSelPhone1.ClientID%>");--%>
            var txtSelPhone2 = $("#<%=txtSelPhone2.ClientID%>");
            var txtSelPhone3 = $("#<%=txtSelPhone3.ClientID%>");

            <%--var txtTelPhone1 = $("#<%=txtTelPhone1.ClientID%>");--%>
            var txtTelPhone2 = $("#<%=txtTelPhone2.ClientID%>");
            var txtTelPhone3 = $("#<%=txtTelPhone3.ClientID%>");
            var pwd = $("#<%=txtPwd.ClientID%>");

            hfContent.val(encodeURIComponent(editor_object.getById["txtContent"].getIR())); //히든필드에 에디터 값 저장


            check = document.getElementById("chk1");
            var box = check.checked;

            if (txtTitle.val == '') {
                alert('제목을 입력해주세요');
                return false;
            }

            if (box != true) {
                alert('약관동의를 확인해주세요.')
                return false;
            }

            if (txtCompanyName.val() == '') {
                alert('회사/기관명을 입력해주세요');
                txtCompanyName.focus();
                return false;
            }

            if (txtBoardWrite.val() == '') {
                alert('담당자명을 입력해주세요');
                txtBoardWrite.focus();
                return false;
            }
            if (editor_object.getById["txtContent"].getIR() == '' || editor_object.getById["txtContent"].getIR() == null || editor_object.getById["txtContent"].getIR() == '&nbsp;' || editor_object.getById["txtContent"].getIR() == '<p><br></p>') {
                alert('내용을 입력해주세요');
                editor_object.getById["txtContent"].exec("FOCUS"); //포커싱
                return false;
            }

            if (txtTelPhone2.val() == '' || txtTelPhone3.val() == '') {
                alert('유선전화 번호를 입력해주세요.');
                txtTelPhone2.focus()
                return false;
            }

            if (txtSelPhone2.val() == '' || txtSelPhone3.val() == '') {
                alert('휴대전화 번호를 입력해주세요.');
                txtSelPhone2.focus()
                return false;
            }
           
            var regeMail = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/;

            if (txtEmail1.val() == '' || txtEmail2.val() == '') {
                alert('E-Mail을 입력해주세요.');
                txtEmail1.focus()
                return false;
            }
            if (!regeMail.test((txtEmail1.val() + '@' + txtEmail2.val()))) {
                alert("잘못된 이메일 형식입니다.");
                txtEmail1.focus();
                return false;
            }
            
            
            if (pwd.val() == '') {
                alert('비밀번호를 입력해주세요.');
                pwd.focus();
                return false;
            }

            var maxFileSize = '<%= ConfigurationManager.AppSettings["UploadFileMaxSize"]%>';
            var fileuploader = $("#<%=fuBoard.ClientID%>");

            if (fileuploader[0].files[0] != null) {
                if (fileuploader[0].files[0].size > maxFileSize) {

                    alert('파일 용량은 10MB보다 작아야 합니다.');
                    return false;
                }
            }

            if (!confirm("저장하시겠습니까?")) {
                return false;
            }
            return true;
        }

        function fnInsertCancel() {
            if (confirm('취소하시겠습니까?')) {
                history.back(1);
                return false;
            }
        }
    </script>

</asp:Content>

